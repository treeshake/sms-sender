{
  "name": "sms-sender",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo -p 4000",
    "dev:check": "next lint && tsc --noEmit",
    "dev:typecheck": "tsc --noEmit",
    "dev:format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "dev:format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "next:lint": "next lint",
    "next:lint:fix": "next lint --fix",
    "next:build": "next build",
    "next:preview": "next build && next start",
    "next:start": "next start",
    "precommit": "npm run dev:typecheck && npm run dev:format:write && npm run next:lint:fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
